const ruleOptions = {
  apple: true, // è‹¹æœæœåŠ¡
  microsoft: true, // å¾®è½¯æœåŠ¡
  google: true, // GoogleæœåŠ¡
  openai: true, // å›½å¤–AIå’ŒGPT
